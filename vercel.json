{
  "version": 2,
  "routes": [
    {
      "src": "/api/memberships",
      "dest": "/functions/getMemberships.js"
    },
    {
      "src": "/api/createMembership",
      "dest": "/functions/createMembership.js"
    },
    {
      "src": "/api/deployNFT",
      "dest": "/functions/deployNFT.js"
    },
    {
      "src": "/api/memberships/:id/image",
      "dest": "/functions/getMembershipImage.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
